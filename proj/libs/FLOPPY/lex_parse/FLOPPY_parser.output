Grammar

    0 $accept: input $end

    1 input: statement ';'

    2 statement: create_statement

    3 create_statement: CREATE TABLE ID opt_volatile '(' column_def_commalist ',' primary_key opt_foreign_key_list ')'

    4 opt_volatile: VOLATILE
    5             | %empty

    6 column_def_commalist: column_def
    7                     | column_def_commalist ',' column_def

    8 column_def: ID column_type opt_column_size

    9 attribute_list: ID
   10               | attribute_list ',' ID

   11 primary_key: PRIMARY KEY '(' attribute_list ')'

   12 opt_foreign_key_list: opt_foreign_key_list ',' foreign_key
   13                     | %empty

   14 foreign_key: FOREIGN KEY '(' attribute_list ')' REFERENCES ID

   15 column_type: INT
   16            | FLOAT
   17            | VARCHAR
   18            | DATETIME
   19            | BOOLEAN

   20 opt_column_size: '(' int_literal ')'
   21                | %empty

   22 int_literal: INTVAL


